{
    "name": "thelabdev/laratlas",
    "description": "A Laravel package for generating mostly used data such as regions, districts, wards, streets and service centers  for Tanzania country",
    "keywords": [
        "thelabdev",
        "Laratlas"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
          "name": "Peter Nyirenda",
          "email": "thelabdevtz@gmail.com",
          "role": "Developer"
        }
    ],

    "require": {

    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },

    "autoload": {
        "psr-4": {
            "Thelabdev\\Laratlas\\": "src/",
            "Thelabdev\\Laratlas\\Database\\Seeds\\": "database/seeds/"
        },
        "files": [
          "src/Helpers/helpers.php"
        ],
        "classmap": [
            "database",
            "database/seeds"
        ]
    },

    "autoload-dev": {
        "psr-4": {
            "Thelabdev\\Laratlas\\Tests\\": "tests/"
        }
    },

    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },

    "extra": {
        "laravel": {
            "providers": [
                "Thelabdev\\Laratlas\\LaratlasServiceProvider"
            ],
            "aliases": {
                "Laratlas": "Thelabdev\\Larastarter\\LarastarterFacade"
            }
        }
    }
}
